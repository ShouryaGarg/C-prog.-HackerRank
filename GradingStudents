#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin >> n;
    for(int i = 0; i < n; i++)
    {
        int grade;
        cin >> grade;
        if(grade >= 38)
        {
            int remainder = grade%5;
            if(remainder >= 3)
            {
                grade += (5 - remainder);
            }
        }
        cout << grade << endl;
    }
}
