#include <bits/stdc++.h>

using namespace std;

int main(){
    int n, s = 0, pattern = 0;
    cin >> n;
    
    for(int i =0; i < n; i++)
    {
        s = n - (i + 1);
        for(int j = 0; j < s; j++)
        {
            cout << " ";
        }
        pattern = i + 1;
        for(int j = 0; j < pattern; j++)
        {
            cout << "#";
        }
        cout << endl;
    }
}
