#include <bits/stdc++.h>
using namespace std;

int main(){
    int query;
    cin>>query;
    for(int i=0;i<query;i++){
        int x,y,z;
        cin>>x>>y>>z;
        if(abs(x-z)<abs(y-z)){
            cout<<"Cat A"<<"\n";
        }
        else if(abs(x-z)>abs(y-z)){
            cout<<"Cat B"<<"\n";
        }
        else{
            cout<<"Mouse C"<<"\n";
        }
    }
}
